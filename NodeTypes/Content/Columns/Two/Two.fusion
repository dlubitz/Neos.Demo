##
# "TwoColumn" element
#
prototype(Neos.Demo:Content.Columns.Two) < prototype(Neos.Neos:ContentComponent) {
    column0 = Neos.Neos:ContentCollection {
        nodePath = 'column0'
    }

    column1 = Neos.Neos:ContentCollection {
        nodePath = 'column1'
    }

    renderer = afx`
        <Neos.Demo:Section.Columns.Three>
            {props.column0}
            {props.column1}
        </Neos.Demo:Section.Columns.Three>
    `
}
